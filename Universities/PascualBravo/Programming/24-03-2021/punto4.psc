Algoritmo Punto_4
	Escribir("Digite la variable A ")
	Leer a
	Escribir("Digite la variable B ")
	Leer b
	
	Escribir("La variable A es: "), a
	Escribir("La variable B es: "), b
	
	variableAuxiliar<- b
	b<-a
	a<-variableAuxiliar
	
	Escribir("La variable A es: "), a
	Escribir("La variable B es: "), b
	
FinAlgoritmo
